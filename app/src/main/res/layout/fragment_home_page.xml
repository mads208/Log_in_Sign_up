<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/greenline"
    tools:context=".general.HomePage" >

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <TextView
            android:id="@+id/dayrate"
            android:layout_width="96dp"
            android:layout_height="44dp"
            android:layout_marginStart="30dp"

            android:layout_marginTop="-20dp"
            android:layout_marginBottom="75dp"
            android:background="@drawable/rounded_text"
            android:backgroundTint="#B6D19C"
            android:gravity="center"
            android:text="daily rating"
            android:textAlignment="center"
            android:textColor="#29311F"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/playlists1"
            app:layout_constraintEnd_toStartOf="@+id/schedules"
            app:layout_constraintStart_toStartOf="@+id/DayRating"
            app:layout_constraintTop_toBottomOf="@+id/DayRating" />

        <ImageView
            android:id="@+id/journaling"
            android:layout_width="186dp"
            android:layout_height="194dp"
            android:layout_marginStart="92dp"
            android:layout_marginTop="-79dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="280dp"
            android:src="@drawable/journalicon"
            app:layout_constraintBottom_toTopOf="@+id/schedule"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.879"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/DayRating"
            android:layout_width="197dp"
            android:layout_height="214dp"
            android:layout_marginStart="55dp"
            android:layout_marginTop="130dp"
            android:layout_marginEnd="40dp"
            android:layout_marginBottom="254dp"
            android:src="@drawable/dayrateicon"
            app:layout_constraintBottom_toTopOf="@+id/playlist"
            app:layout_constraintEnd_toStartOf="@+id/journal"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/schedules"
            android:layout_width="176dp"
            android:layout_height="174dp"
            android:layout_marginStart="45dp"
            android:layout_marginTop="-45dp"
            android:layout_marginBottom="207dp"
            android:src="@drawable/schedualicon"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/dayrate"
            app:layout_constraintTop_toBottomOf="@+id/journal" />

        <ImageView
            android:id="@+id/playlists1"
            android:layout_width="186dp"
            android:layout_height="204dp"
            android:layout_marginEnd="198dp"
            android:layout_marginBottom="55dp"
            android:src="@drawable/musicicon"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/dayrate" />

        <TextView
            android:id="@+id/journal"
            android:layout_width="89dp"
            android:layout_height="42dp"

            android:layout_marginTop="-88dp"
            android:layout_marginEnd="115dp"
            android:layout_marginBottom="80dp"
            android:background="@drawable/rounded_text"
            android:backgroundTint="#B6D19C"
            android:gravity="center"
            android:text="journaling"
            android:textAlignment="center"
            android:textColor="#29311F"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/schedules"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/DayRating"
            app:layout_constraintTop_toBottomOf="@+id/journaling"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/schedule"
            android:layout_width="106dp"
            android:layout_height="35dp"
            android:layout_marginStart="-10dp"
            android:layout_marginTop="-30dp"
            android:layout_marginEnd="5dp"
            android:layout_marginBottom="140dp"
            android:background="@drawable/rounded_text"
            android:backgroundTint="#B6D19C"

            android:gravity="center"
            android:text="schedule"
            android:textAlignment="center"
            android:textColor="#29311F"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/schedules"
            app:layout_constraintHorizontal_bias="0.693"
            app:layout_constraintStart_toStartOf="@+id/schedules"
            app:layout_constraintTop_toBottomOf="@+id/journaling" />

        <TextView
            android:id="@+id/playlist"
            android:layout_width="105dp"
            android:layout_height="36dp"
            android:layout_marginTop="40dp"
            android:layout_marginBottom="-40dp"
            android:background="@drawable/rounded_text"
            android:backgroundTint="#B6D19C"
            android:gravity="center"

            android:text="playlists"
            android:textAlignment="center"
            android:textColor="#29311F"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/playlists1"
            app:layout_constraintHorizontal_bias="0.518"
            app:layout_constraintStart_toStartOf="@+id/playlists1"
            app:layout_constraintTop_toBottomOf="@+id/DayRating" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>