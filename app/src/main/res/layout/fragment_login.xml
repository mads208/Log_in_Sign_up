<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#3C6E41"
    tools:context=".LoginFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="412dp"
        android:layout_height="847dp">


        <ImageView
            android:id="@+id/background"
            android:layout_width="589dp"
            android:layout_height="978dp"
            android:layout_marginTop="-30dp"
            android:contentDescription="background"
            android:src="@drawable/backgroundblues"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/square" />

        <ImageView
            android:id="@+id/blues"
            android:layout_width="456dp"
            android:layout_height="0dp"
            android:layout_marginStart="68dp"
            android:layout_marginTop="-30dp"
            android:contentDescription="blues "
            android:src="@drawable/blues"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/background"
            app:layout_constraintTop_toTopOf="parent" />


        <ImageView
            android:id="@+id/circle9"
            android:layout_width="73dp"
            android:layout_height="79dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="112dp"
            android:src="@drawable/dark_circle"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/circle1"
            android:layout_width="63dp"
            android:layout_height="71dp"
            android:layout_marginStart="28dp"
            android:layout_marginTop="116dp"
            android:src="@drawable/circle"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/square"
            android:layout_width="0dp"
            android:layout_height="750dp"
            android:layout_marginStart="55dp"
            android:layout_marginEnd="57dp"
            android:layout_marginBottom="179dp"
            android:contentDescription="square"
            android:src="@drawable/rounded_square"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/blues" />

        <ImageView
            android:id="@+id/circle8"
            android:layout_width="44dp"
            android:layout_height="46dp"
            android:layout_marginStart="57dp"
            android:layout_marginTop="115dp"
            android:src="@drawable/dark_circle"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/circle2"
            android:layout_width="47dp"
            android:layout_height="58dp"
            android:layout_marginStart="64dp"
            android:layout_marginTop="104dp"
            android:src="@drawable/circle_dark"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/circle11"
            android:layout_width="63dp"
            android:layout_height="61dp"
            android:layout_marginTop="47dp"
            android:layout_marginEnd="58dp"
            android:src="@drawable/dark_circle"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/circle3"
            android:layout_width="52dp"
            android:layout_height="52dp"
            android:layout_marginTop="52dp"
            android:layout_marginEnd="64dp"
            android:src="@drawable/circle"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/circle7"
            android:layout_width="54dp"
            android:layout_height="59dp"
            android:layout_marginTop="64dp"
            android:layout_marginEnd="82dp"
            android:src="@drawable/dark_circle"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/circle4"
            android:layout_width="45dp"
            android:layout_height="44dp"
            android:layout_marginTop="72dp"
            android:layout_marginEnd="88dp"
            android:src="@drawable/circle_dark"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/circle10"
            android:layout_width="70dp"
            android:layout_height="75dp"
            android:layout_marginEnd="28dp"
            android:layout_marginBottom="388dp"
            android:src="@drawable/dark_circle"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <ImageView
            android:id="@+id/circle5"
            android:layout_width="58dp"
            android:layout_height="58dp"
            android:layout_marginTop="392dp"
            android:layout_marginEnd="32dp"
            android:src="@drawable/circle_dark"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/etUsernameLogin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="92dp"
            android:layout_marginTop="164dp"
            android:layout_marginBottom="42dp"
            android:ems="10"
            android:hint="Email / UserName ..."
            android:inputType="textEmailAddress"
            android:minHeight="48dp"
            app:layout_constraintBottom_toTopOf="@+id/etPasswordLogin"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/etPasswordLogin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="81dp"
            android:ems="10"
            android:hint="Password ..."
            android:inputType="textPassword"
            android:minHeight="48dp"
            app:layout_constraintBottom_toTopOf="@+id/btnLoginLogin"
            app:layout_constraintStart_toStartOf="@+id/etUsernameLogin"
            app:layout_constraintTop_toBottomOf="@+id/etUsernameLogin" />

        <Button
            android:id="@+id/btnLoginLogin"
            android:layout_width="200dp"
            android:layout_height="0dp"
            android:layout_marginBottom="414dp"
            android:backgroundTint="#D4F2D4"
            android:drawableTint="#1B1A1A"
            android:text="Log in"
            android:textColor="#2A562C"
            android:textColorHint="#020202"
            android:textColorLink="#181111"
            app:iconTint="#FFFFFF"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/square"
            app:layout_constraintStart_toStartOf="@+id/square"
            app:layout_constraintTop_toBottomOf="@+id/etPasswordLogin"
            app:rippleColor="#0F0B0B" />

        <TextView
            android:id="@+id/ForgotPasswordLogin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="212dp"
            android:background="@drawable/rounded_text"
            android:text="   Forgot Password ?   "
            android:textColor="#29311F"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.468"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/tvSignupLinkLogin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="28dp"
            android:layout_marginBottom="64dp"
            android:background="@drawable/rounded_text"
            android:backgroundTint="#91A878"
            android:text="  Sign up ...   "
            android:textColor="#29311F"
            app:layout_constraintBottom_toTopOf="@+id/ForgotPasswordLogin"
            app:layout_constraintStart_toStartOf="@+id/ForgotPasswordLogin" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/GoToAddLogin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="43dp"
        android:text="Add Data ..."
        android:textColor="#FFFFFF"
        android:visibility="invisible"
        tools:visibility="gone" />

</FrameLayout>